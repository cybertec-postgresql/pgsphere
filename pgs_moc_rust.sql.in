CREATE FUNCTION smoc_disc_rust ("order" int, lng double precision, lat double precision, radius double precision)
	RETURNS smoc
	AS 'MODULE_PATHNAME'
	LANGUAGE C
	PARALLEL SAFE
	IMMUTABLE
	STRICT;

COMMENT ON FUNCTION smoc_disc_rust ("order" int, lng double precision, lat double precision, radius double precision)
	IS 'constructs smoc of given order from circle parameters';

